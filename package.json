{
  "name": "poly.js",
  "version": "0.0.1",
  "main": "build/src/index.js",
  "scripts": {
    "start": "yarn go && yarn build && node ./build/src/index",
    "build": "rm -rf ./build && tsc",
    "dev": "yarn go && yarn dev:run",
    "dev:run": "ts-node src/index",
    "go": "yarn go:clean && yarn go:build",
    "go:build": "gopherjs build -o ./src/index.js",
    "go:clean": "rm -rf ./src/*.{js, map}",
    "test": "ts-node ./tests/genbank.test",
    "types:generate": "tygo generate"
  },
  "author": "isaacguerreir <isaacguerreirocom@gmail.com>",
  "license": "MIT",
  "keywords": [
    "bioinformatics",
    "synthetic-biology",
    "synbio",
    "genetic-engineering"
  ],
  "devDependencies": {
    "@types/node": "^18.11.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
